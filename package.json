{
  "name": "autonomous-bounty-bot",
  "version": "2.0.0",
  "description": "Fully autonomous bounty bot for POIDH V3 - creates, monitors, evaluates, and pays out real-world proof bounties on Base Mainnet",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/agent.js",
    "dev": "npx ts-node src/index.ts",
    "agent": "npx ts-node src/agent.ts",
    "agent:outside": "npx ts-node src/agent.ts proveOutside",
    "agent:handwritten": "npx ts-node src/agent.ts handwrittenDate",
    "agent:meal": "npx ts-node src/agent.ts mealPhoto",
    "agent:tower": "npx ts-node src/agent.ts objectTower",
    "agent:shadow": "npx ts-node src/agent.ts shadowArt",
    "agent:animal": "npx ts-node src/agent.ts animalPhoto",
    "agent:list": "npx ts-node src/agent.ts list",
    "agent:monitor": "npx ts-node src/agent.ts monitor",
    "demo:first-valid": "npx ts-node src/demos/first-valid-demo.ts",
    "demo:ai-judged": "npx ts-node src/demos/ai-judged-demo.ts",
    "demo:full": "npx ts-node src/demos/full-demo.ts",
    "demo:simulate": "npx ts-node src/demos/simulation-demo.ts",
    "wallet:create": "npx ts-node src/scripts/create-wallet.ts",
    "wallet:balance": "npx ts-node src/scripts/check-balance.ts",
    "bounty:cancel": "npx ts-node src/scripts/cancel-bounty.ts",
    "bounty:list": "npx ts-node src/scripts/list-bounties.ts",
    "bounty:claims": "npx ts-node src/scripts/check-claims.ts",
    "bounty:continuous": "npx ts-node src/scripts/continuous-bounty-loop.ts",
    "bounty:close-all": "npx ts-node src/scripts/close-all-bounties.ts",
    "bounty:deploy-test": "npx ts-node src/scripts/deploy-test-bounty.ts",
    "audit:validate": "npx ts-node src/scripts/validate-audit-logs.ts",
    "server:stream": "npx ts-node src/server/streaming-server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage logs",
    "gui": "python gui/poidh_gui.py",
    "gui:build": "cd gui && bash build.sh",
    "gui:build:win": "cd gui && build.bat",
    "gui:package": "bash gui/installer/build-all.sh",
    "gui:package:win": "gui\\installer\\build-all.bat",
    "gui:installer:macos": "bash gui/installer/macos.sh",
    "gui:installer:linux": "bash gui/installer/linux.sh",
    "cli": "python gui/cli.py"
  },
  "dependencies": {
    "@types/express": "^5.0.6",
    "@types/ws": "^8.18.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "exif-reader": "^2.0.0",
    "express": "^4.22.1",
    "node-cron": "^3.0.3",
    "openai": "^4.24.0",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drdeek/poidh-autonomous.git"
  },
  "author": "DrDeek",
  "keywords": [
    "poidh",
    "bounty",
    "autonomous",
    "ai",
    "blockchain",
    "base",
    "farcaster",
    "gpt4-vision",
    "web3",
    "ethereum"
  ],
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
